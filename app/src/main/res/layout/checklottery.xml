        android:background="@color/seach_num_lot"
        android:drawableLeft="@drawable/ic_search_b"
        android:textColorHint="@color/text_hint"
        android:ems="6"
        android:gravity="center"
        android:hint="ค้นหาผลสลากกินแบ่งรัฐบาล"
        android:inputType="number"
        android:maxLength="6"
        android:focusableInTouchMode="true"
        />


